(set-option :pp.decimal true)
(set-option :pp.decimal-precision 20)

(simplify (+ (^ 3.0 (/ 1.0 2.0)) (^ 5.0 (/ 1.0 2.0))))
(simplify (^ (* 2.0 (+ 4.0 (^ 15.0 (/ 1.0 2.0)))) (/ 1.0 2.0)))
(simplify (= (+ (^ 3.0 (/ 1.0 2.0)) (^ 5.0 (/ 1.0 2.0)))
             (^ (* 2.0 (+ 4.0 (^ 15.0 (/ 1.0 2.0)))) (/ 1.0 2.0))))

(simplify (- (^ 3.0 (/ 1.0 2.0)) (^ 5.0 (/ 1.0 2.0))))
(simplify (- (^ (* 2.0 (- 4.0 (^ 15.0 (/ 1.0 2.0)))) (/ 1.0 2.0))))
(simplify (= (- (^ 3.0 (/ 1.0 2.0)) (^ 5.0 (/ 1.0 2.0)))
             (- (^ (* 2.0 (- 4.0 (^ 15.0 (/ 1.0 2.0)))) (/ 1.0 2.0)))))

(simplify (- (^ 3.0 (/ 1.0 2.0)) (* 2.0 (^ 2.0 (/ 1.0 2.0)))))
(simplify (- (^ (- 11.0 (* 4.0 (^ 6.0 (/ 1.0 2.0)))) (/ 1.0 2.0))))
(simplify (= (- (^ 3.0 (/ 1.0 2.0)) (* 2.0 (^ 2.0 (/ 1.0 2.0))))
             (- (^ (- 11.0 (* 4.0 (^ 6.0 (/ 1.0 2.0)))) (/ 1.0 2.0)))))

(simplify (^ 5.0 (/ 125.0 100.0)))